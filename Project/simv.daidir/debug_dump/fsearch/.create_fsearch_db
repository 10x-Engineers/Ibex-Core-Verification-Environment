export LD_LIBRARY_PATH
/usr/synopsys/vcs-L-2016.06/linux64/bin/vcsfind_create_index.exe  -z  "simv.daidir/debug_dump/fsearch/./idents_UrqM19.xml.gz" -o "simv.daidir/debug_dump/fsearch/fsearch.db_tmp"
\mv "simv.daidir/debug_dump/fsearch/fsearch.db_tmp" "simv.daidir/debug_dump/fsearch/fsearch.db"
