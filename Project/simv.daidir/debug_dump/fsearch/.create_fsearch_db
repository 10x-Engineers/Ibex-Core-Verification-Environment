#!/bin/sh -h 
PYTHONHOME=/usr/synopsys/vcs-L-2016.06//etc/search/pyh
export PYTHONHOME
PYTHONPATH=/usr/synopsys/vcs-L-2016.06/linux64/lib/pylib27
export PYTHONPATH
